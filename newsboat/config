auto-reload yes
reload-threads 5

highlight feedlist "---.*---" cyan default bold
highlight feedlist ".*0/0.." default default invis

browser "qutebrowser %u"

# vim binds
# unbinds
unbind-key ENTER
unbind-key j
unbind-key k
unbind-key J
unbind-key K

# binds
bind-key j down
bind-key k up
bind-key l open
bind-key h quit

