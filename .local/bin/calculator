# disable history
import readline
readline.write_history_file = lambda *args: None

import sysconfig
# we import two times so we can enter help(math) and don't specify namespaces
# for math functions at the same time
import math
from math import *
print(f"Python {sysconfig.get_python_version()} with module math imported.")
print(f"Enter help() for general help and help(math) for help on mathematical functions.")
print(f"Constants: pi, e, tau, inf, nan.")
